<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <script src="/socket.io/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
    <script rel='javascript' src='/js/messages.js'></script>
    <link rel='stylesheet' href='/css/navbar.css'/>
    <link rel="stylesheet" href="/assets/css/main.css" />  </head>
  <body>
    <span class="spanTag" id="<%= user.name %>"></span>
    <div id="wrapper">

      <%- include('../partials/navbar.ejs')  %>

      <div id="main">
        <div class="inner">
          <section>
            <div class="row">
              <div class="6u$ 12u$(medium)"></div>
            </div>
            <div class="row">
              <div class="12u 12u$(medium)">
                <ul class="alt" id="messages"></ul>
                <form id="form">
                  <input type="text" name="demo-name" id="m" autocomplete="off" placeholder="Enter your message"/>
                  <br>
                  <button class="button special">Send Message</button>
                </form>
              </div>
            </div>
          </section>
        </div>
      </div>

      <%- include('../partials/footer.ejs')  %>

    </div>

    <script src="/assets/js/jquery.min.js"></script>
    <script src="/assets/js/skel.min.js"></script>
    <script src="/assets/js/util.js"></script>
    <script src="/assets/js/main.js"></script>
  </body>
</html>
